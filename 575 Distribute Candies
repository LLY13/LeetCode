public class Solution {
    public int distributeCandies(int[] candies) {
    	int leng = candies.length;
    	Set<Integer> set = new HashSet<>();
        for (int i : candies) {
			set.add(i);
		}
        return (set.size() < leng/2 ? set.size() : leng/2);
    }
}

//understand and use Set  HashSet
