public class Solution {
    public List<String> fizzBuzz(int n) {
        List<String> out = new LinkedList<>();
        for(int i=1;i<=n;i++){
        	String tem;
        	if(i%3==0&&i%5==0){
        		tem = "FizzBuzz";
        	}else if (i%3==0&&i%5!=0) {
				tem = "Fizz";
			}else if (i%3!=0&&i%5==0) {                             //mysolution
				tem = "Buzz";
			}else {
				tem = "" + i;
			}
        	out.add(tem);
        }
        return out;
    }
}





